name = Korora Starter
description = Install profile for a quick gettind started D7 project.
version = 1.0
core = 7.x

;=== Settings (see profile's hook_install_tasks) ==============================
; Timezone and Language
-----------------------
settings[default_timezone] = "Europe/Paris"
settings[default_country] = "FR"
settings[default_language] = "fr"
settings[first_day] = 
settings[import_translations] = 1

; Admin theme
-------------
settings[admin_theme] = adminimal
;settings[admin_theme] = rubik

; Front theme
-------------
settings[default_theme] = "bartik"
; Do not forget to activate the corresponding theme dependency in this .info file
; and the corresponding inclusion in the drupal-org.make file.

; Site
settings[site_mail] = "admin@example.com"
settings[site_name] = "Korora starter Site"
settings[site_slogan] = "Korora helps you to start your project faster"

; Admin account name
settings[user1_name] = "superadmin"

